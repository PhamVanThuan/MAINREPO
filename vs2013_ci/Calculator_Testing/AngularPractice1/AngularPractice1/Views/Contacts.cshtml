@{
    PageData["title"] = "AngularJS SPA | Contacts";
}

<html>
<head>
    <title>
        Contact Us
    </title>
    <script data-require="angular.js@" data-semver="1.3.13" src="https://code.angularjs.org/1.3.13/angular.js"></script>
    <script src="MyNewScript.js"></script>
</head>

<body>
    <h2>Contact Us</h2>
    <div ng-controller="MainController">
         
        <!-- Populate fields with an object in MyNewScript.js-->
        <input type="text" required placeholder="Enter your name" name="Name" />
        <input type="email" required placeholder="xxxxx@xxx.com OR xxxx@domain" name="Email"/>
        <time>{{message}}</time>

    </div>

</body>
</html>