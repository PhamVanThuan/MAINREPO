<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>

<html>
	<head>
		<title><#=Service.ClearName#></title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link href="../css/bootstrap.min.css" rel="stylesheet" media="screen">
		<style>
			ul li a{color:#ff6600}
			ul li a:hover{color:#e65c00}
			.nav-list>.active>a,.nav-list>.active>a:hover,.nav-list>.active>a:focus{background-color:#ff6600}
		</style>
	</head>
	<body>
	<div class="container-fluid">
		<div class="row">
			<div class="span12">
				<h1><#=Service.ClearName#> Domain Service</h1>
			</div>
		</div>
		<div class="row-fluid">
			<div class="span3">
				<ul class="nav nav-list well">
				  <li class="nav-header">Documentation</li>
				  <li><a href="../index.html">Home</a></li>
				  <li><a href="../commands.html"">Commands</a></li>
				  <li><a href="../events.html"">Events</a></li>
					<ul class="nav nav-list">
				<# foreach(var evt in EventModels.OrderBy(x=>x.Name))
				{#>
						<li <#if (evt.Name == EventModel.Name){#>class="active"<#}#>>
							<a href="../events/<#=evt.Name.ToLower()#>.html"><#=evt.ClearName#></a>
						</li>
				<#}#>
					</ul>
				  <li><a href="../models.html"">Models</a></li>
				  <li><a href="../queries.html"">Queries</a></li>
				  <li><a href="../rules.html"">Rules</a></li>
				  <li><a href="../domainchecks.html"">Domain Checks</a></li>
				  <li><a href="../enumerators.html"">Enumerators</a></li>
				</ul>
			</div>
			<div class="span9">
				<h2><#=EventModel.ClearName#></h2>

				<h3>Properties</h3>
				<#if(EventModel.Properties.Count() > 0)
				{#>
					<table class="table table-striped">
						<thead>
						<tr>
							<th>Name</th>
							<th>Type</th>
						<tr>
						</thead
						<tbody>
				<# foreach(var prop in EventModel.Properties.OrderBy(x=>x.Name))
				{#>
						<tr>
							<td><#=prop.ClearName#></td>
					<#if(prop.IsSAHLModel)
					{#>
							<td><a href="../models/<#=prop.Type#>.html"><#=prop.ClearType#></td>
					<#}
					else if(prop.IsCollectionSAHLModel)
					{#>
							<td><a href="../models/<#=prop.CollectionPropertyType#>.html"><#=prop.Type#></td>
					<#}
					else if(prop.IsSAHLEnum)
					{#>
							<td><a href="../enumerators/<#=prop.Type#>.html"><#=prop.ClearType#></td>
					<#} else {#>
							<td><#=prop.Type#></td>
					<#}#>
						</tr>
				<#}#>			
						</tbody>
					</table>
				<#} else {#>
					<table class="table table-striped">
						<tbody>
							<tr>
								<td>
									<b>No Properties</b>
								</td>
							</tr>
						</tbody>
					</table>
				<#}#>


				<h3>Parent Commands</h3>
				<#if(EventModel.ParentCommands.Count() > 0)
				{#>
					<table class="table table-striped">
						<thead>
						<tr>
							<th>Name</th>
						<tr>
						</thead
						<tbody>
				<# foreach(var parentCommand in EventModel.ParentCommands.OrderBy(x=>x.Name))
				{#>
						<tr>
							<td><a href="../commands/<#=parentCommand.Name#>.html"><#=parentCommand.ClearName#></a></td>
						</tr>
				<#}#>			
						</tbody>
				</table>
				<#} else {#>
				<table class="table table-striped">
						<tbody>
							<tr>
								<td>
									<b>No Parent Commands</b>
								</td>
							</tr>
						</tbody>
				</table>
				<#}#>
			</div>
		</div>
	</div>
	</body>
</html>