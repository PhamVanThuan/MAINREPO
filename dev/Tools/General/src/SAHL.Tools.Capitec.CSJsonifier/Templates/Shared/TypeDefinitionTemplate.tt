<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>
function <#=this.Model.Name#>(<#= this.TemplateManager.GetTemplate(this.Constructor).Process(this.Constructor) #>) {
		<#foreach(var parameter in this.Constructor.Parameters){#>
		<#=this.TemplateManager.GetTemplate(parameter).Process(parameter)#>
		<#}#>
		this._name = '<#=this.Model.FullName#>,<#=this.Model.Module.Assembly.Name.Name#>';
		<# if(this.ValidationAttributes.Count() > 0) { #>
		this.Validate = function() {
					var results = [];
<#foreach(var validationAttribute in this.ValidationAttributes) {#>
					<#=this.TemplateManager.GetTemplate(validationAttribute).Process(validationAttribute)#>
<# } #>
					return results;
			};
<# } #>
		}