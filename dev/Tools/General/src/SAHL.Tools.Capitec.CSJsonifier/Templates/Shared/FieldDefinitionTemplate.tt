<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>
<#@ import namespace="Mono.Cecil" #>
<#if(this.ModelType!= null && this.ModelType.IsClass && this.Model.IsPublic){#><#=this.Model.Name#> : {
<#this.PushIndent("\t");#>
<#foreach(PropertyDefinition property in this.GetProperties()){#>
	<#= this.TemplateManager.GetTemplate(property).Process(property)#><#if(this.GetProperties().Last() != property || GetFields().Count() > 0){#>,
<#}else{#>

<#}}#>
<#this.PopIndent();#>
<#foreach(FieldDefinition field in GetFields()){#>
<#this.PushIndent("\t");#>
	<#= this.TemplateManager.GetTemplate(field).Process(field)#><#if(this.GetFields().Last() != field){#>,<#}#>

<#this.PopIndent();#>
<#}#>
	}<#}#>