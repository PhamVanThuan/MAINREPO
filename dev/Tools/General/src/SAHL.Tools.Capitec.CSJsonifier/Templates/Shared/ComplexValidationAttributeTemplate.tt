if(!this.<#=this.Model.PropertyName#>){
							results.push({'<#=this.Model.PropertyName#>': '<#=this.Model.PropertyNameText#> is required'});
						} else if(!this.<#=this.Model.PropertyName#>['name'] || this.<#=this.Model.PropertyName#>.name !== '<#= this.Model.FullName #>'){
							results.push({'<#=this.Model.PropertyName#>': '<#=this.Model.PropertyNameText#> must be of type <#= this.Model.FullName #>'});
						} else {
							if(this.<#=this.Model.PropertyName#>.Validate)
								results.push(this.<#=this.Model.PropertyName#>.Validate());
						}
