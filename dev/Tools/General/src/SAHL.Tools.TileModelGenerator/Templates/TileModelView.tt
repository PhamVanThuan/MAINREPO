<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>
<div class="labels">
<#foreach(KeyValuePair<string,string> kvp in this.Model.Properties)
{#>	<div><#= kvp.Value #>:</div>
<#}#>
</div>
<div class="values">
<#foreach(KeyValuePair<string,string> kvp in this.Model.Properties)
{#>	<div>{{item.tileData.<#= kvp.Key #>}}</div>
<#}#>
</div>