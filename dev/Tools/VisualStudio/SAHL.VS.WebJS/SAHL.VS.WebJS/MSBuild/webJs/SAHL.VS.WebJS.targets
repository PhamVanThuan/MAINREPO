<?xml version="1.0" encoding="utf-8" ?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <UsingTask AssemblyFile="bin\SAHL.VS.Build.dll" TaskName="WebJsLint" />
  <Target  Name="GetFrameworkPaths">
  </Target>
  <Target  Name="ResolveAssemblyReferences">
  </Target>
  <Target  Name="Clean">
  </Target>
  <Target  Name="Build" DependsOnTargets="GetFrameworkPaths;ResolveAssemblyReferences">
    <WebJsLint ProjectDir="$(MSBuildProjectDirectory)" RepoRootPath="$(MSBuildProjectDirectory)\$(SAHLSourceRoot)"></WebJsLint>
  </Target>
  <Target  Name="ReBuild" DependsOnTargets="Clean;Build">
  </Target>
</Project>
