<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<system.serviceModel>
		<bindings>
   <basicHttpBinding>
    <binding name="SAHL.WCFServices.ComcorpConnector.Server.SAHLIntegrationService" />
    <binding name="SAHLDocumentService">
     <security mode="None">
      <transport clientCredentialType="None" />
      <message clientCredentialType="UserName" />
     </security>
    </binding>
   </basicHttpBinding>
  </bindings>
		<client>
   <endpoint address="http://localhost/SAHLComcorpConnectorService/SAHLIntegrationService.svc/integration"
    binding="basicHttpBinding" bindingConfiguration="SAHL.WCFServices.ComcorpConnector.Server.SAHLIntegrationService"
    contract="SAHLIntegration.ISAHLIntegrationService" name="SAHL.WCFServices.ComcorpConnector.Server.SAHLIntegrationService" />
   <endpoint address="http://localhost/SAHLComcorpConnectorService/SAHLIntegrationService.svc/document"
    binding="basicHttpBinding" bindingConfiguration="SAHLDocumentService"
    contract="SAHLIntegration.ISAHLDocumentService" name="SAHLDocumentService" />
  </client>
	</system.serviceModel>
</configuration>