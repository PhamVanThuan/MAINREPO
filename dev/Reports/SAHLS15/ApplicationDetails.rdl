    <DataSet Name="ActualLoanDetails">
      <Query>
        <DataSourceName>Real_time</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OfferKey">
            <Value>=Parameters!OfferKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	O.OfferKey,
		'NewPurchase' =
			CASE
				WHEN O.OfferTypeKey = 7 THEN 1
					ELSE 0
			END,
		'LoanPurpose' = Otyp.Description,
		'Product' = Prod.Description,
		'FixedPercentage' = ISNULL(Ff.FixPercentage, 0),
		'InterestOnly' = 
			CASE
				WHEN oifa.OfferInformationFinancialAdjustmentKey is not null THEN 'Yes'
					ELSE 'No'
			END,
		'ApplicantType' =
			CASE
				WHEN ISNULL(Le.RegistrationNumber, '') = '' THEN 'Natural Person'
					ELSE Let.Description
			END,
		'RegistrationNumber' = ISNULL(Le.RegistrationNumber, ''),
		'NameRegistered' = [2AM].dbo.OfferLegalName(O.OfferKey, 1, 0),
		'OutstandingLoan' = CONVERT(NUMERIC(13, 2), ISNULL(Oivl.ExistingLoan, 0)) + CONVERT(NUMERIC(13, 2), ISNULL(Oivl.InterimInterest, 0)),
		'PurchasePrice' = CONVERT(NUMERIC(13, 2), ISNULL(oml.PurchasePrice, 0)),
		'CashOut' = CONVERT(NUMERIC(13, 2), ISNULL(Oivl.RequestedCashAmount, 0)),
		'Deposit' = CONVERT(NUMERIC(13, 2), ISNULL(Oivl.CashDeposit, 0)),
		'TotalRequirement' = CONVERT(NUMERIC(13, 2), ISNULL(LoanAgreementAmount, 0)),
		'RegisteredAmount' = CONVERT(NUMERIC(13, 2), ISNULL(BondToRegister, 0)),
		'LoanTerm' = Oivl.Term

FROM	[2AM].dbo.Offer O

		INNER JOIN [2AM].dbo.OfferType Otyp ON
			(
				O.OfferTypeKey = Otyp.OfferTypeKey
			)
		INNER JOIN [2AM].dbo.OfferMortgageLoan oml ON
			(
				o.OfferKey = oml.OfferKey
			)
		INNER JOIN (SELECT MAX(OfferInformationKey) as OIKey, OfferKey 
				FROM OfferInformation 
				GROUP BY OfferKey) OIKeys ON O.OfferKey=OIKeys.OfferKey 
		
		INNER JOIN [2AM].dbo.OfferInformation Oi ON
			(
				OIKeys.OIKey = OI.OfferInformationKey
					AND Oi.OfferInformationTypeKey = 3
			)
		LEFT JOIN [2am].dbo.OfferInformationFinancialAdjustment oifa on
			oifa.OfferInformationKey = OIKeys.OIKey and oifa.FinancialAdjustmentTypeSourceKey in(
				select FinancialAdjustmentTypeSourceKey 
				from [2am].fin.FinancialAdjustmentTypeSource 
				where FinancialAdjustmentTypeKey = 6 and FinancialAdjustmentSourceKey = 6 --Interest Only
			)
		INNER JOIN [2AM].dbo.Product Prod ON
			(
				Oi.ProductKey = Prod.ProductKey
			)

		LEFT OUTER JOIN SAHLDB.dbo.FlexiFix Ff ON
			(
				Oi.OfferInformationKey = Ff.OfferInformationKey
			)

		LEFT OUTER JOIN [2AM].dbo.OfferInformationInterestOnly Oiio ON
			(
				Oi.OfferInformationKey = Oiio.OfferInformationKey
			)
		INNER JOIN [2AM].dbo.OfferInformationVariableLoan Oivl ON
			(
				Oi.OfferInformationKey = Oivl.OfferInformationKey
			)

		LEFT OUTER JOIN (SELECT	FirstNonNatLe.OfferKey,
								i.LegalEntityKey,
								i.RegistrationNumber,
								i.LegalEntityTypeKey
						FROM	(SELECT	Orl.OfferKey,
										'LegalEntityKey' =  MIN(Orl.LegalEntityKey)
								FROM	[2AM].dbo.OfferRole Orl
										INNER JOIN [2AM].dbo.LegalEntity Le ON
											(
												Orl.LegalEntityKey = Le.LegalEntityKey
													AND Le.LegalEntityTypeKey &lt;&gt; 2
											)
								WHERE	Orl.OfferRoleTypeKey = 11
										AND Orl.GeneralStatusKey = 1
								GROUP BY Orl.OfferKey)	FirstNonNatLe

								INNER JOIN [2AM].dbo.LegalEntity i ON
									(
										FirstNonNatLe.LegalEntityKey = i.LegalEntityKey
									)) Le ON
			(
				O.OfferKey = Le.OfferKey
			)
		LEFT OUTER JOIN [2AM].dbo.LegalEntityType Let ON
			(
				Le.LegalEntityTypeKey = Let.LegalEntityTypeKey
			)
WHERE O.OfferKey = @OfferKey</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
