    <DataSet Name="LoanDetails">
      <Query>
        <DataSourceName>Real_time</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OfferKey">
            <Value>=Parameters!OfferKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	TOP 1 O.OfferKey,
		'IncomeBasic' = CONVERT(NUMERIC(13, 2),
			CASE
				WHEN Emp.ConfirmedBasicIncome &gt; 0 THEN Emp.ConfirmedBasicIncome
				WHEN Emp.ConfirmedIncome &gt; 0 THEN Emp.ConfirmedIncome
				WHEN Emp.BasicIncome &gt; 0 THEN Emp.BasicIncome
				WHEN Emp.MonthlyIncome &gt; 0 THEN Emp.MonthlyIncome
			END),
		'IncomeCommission' = CONVERT(NUMERIC(13, 2),
			CASE
				WHEN Emp.ConfirmedCommission = 0 THEN Emp.Commission
					ELSE Emp.ConfirmedCommission
			END),
		'IncomeOvertime' = CONVERT(NUMERIC(13, 2),
			CASE
				WHEN Emp.ConfirmedOvertime = 0 THEN Emp.Overtime
					ELSE Emp.ConfirmedOvertime
			END),
		'IncomeInvestment' = CONVERT(NUMERIC(13, 2), 0),
		'IncomeOther1' = CONVERT(NUMERIC(13, 2),
			CASE
				WHEN Emp.ConfirmedOther = 0 THEN Emp.Other
					ELSE Emp.ConfirmedOther
			END),
		'IncomeOther2' = CONVERT(NUMERIC(13, 2), 0),
		'StandardDeductions' = CONVERT(NUMERIC(13, 2), 0),
		'LivingExpenses' = CONVERT(NUMERIC(13, 2), 0),
		'HomeLoanPayments' = CONVERT(NUMERIC(13, 2), ISNULL(ExistLoan.MonthlyPayment, 0)),
		'CarRepayments' = CONVERT(NUMERIC(13, 2), ISNULL(CarFin.MonthlyPayment, 0)),
		'CreditCards' = CONVERT(NUMERIC(13, 2), 0),
		'OverdraftPersonalLoans' = CONVERT(NUMERIC(13, 2), 0),
		'RetailAccounts' = CONVERT(NUMERIC(13, 2), ISNULL(Retail.MonthlyPayment, 0)),
		'CreditAccounts' = CONVERT(NUMERIC(13, 2), 0),
		'CommittedSavings' = CONVERT(NUMERIC(13, 2), 0),
		'OtherInstalment' = CONVERT(NUMERIC(13, 2), 0),
		'OtherExpense' = CONVERT(NUMERIC(13, 2), ISNULL(OtherExp.MonthlyPayment, 0)),
		'TitleType' = Ttype.Description,
		'PropertyType' = Ptype.Description,
		'OccupancyType' = Otype.Description,
		'PropertyAddress' = [2AM].dbo.fGetFormattedAddressDelimited(Prop.AddressKey, 1),
		'PropertyDescription' = ISNULL(Prop.PropertyDescription1 + ' ', '') + ISNULL(Prop.PropertyDescription2 + ' ', '') + ISNULL(Prop.PropertyDescription3 + ' ', ''),
		'MarketValue' = CONVERT(NUMERIC(13, 2), ISNULL(Val.ValuationAmount, 0)),
		'SellerID' = ISNULL(SellerLe.IDNumber, ISNULL(SellerLe.PassportNumber, '')),
		'ValuationContactName' = ISNULL(Valuer.ValuatorContact, ''),
		'ValuationContactNumber' = ISNULL('(' + ValuerLe.WorkPhoneCode + ') ', '') + ISNULL(ValuerLe.WorkPhoneNumber, ''),
		'PropertyContact1' = pad.Contact1,
		'PropertyContact1Phone' = pad.Contact1Phone,
		'PropertyContact1WorkPhone' = pad.Contact1WorkPhone,
		'PropertyContact1MobilePhone' = pad.Contact1MobilePhone,
		'PropertyContact2' = pad.Contact2,
		'PropertyContact2Phone' = pad.Contact2Phone,
		'DebitOrder' =
			CASE
				WHEN Odo.OfferKey IS NULL THEN 0
					ELSE 1
			END,
		'PaymentMethod' = ISNULL(Fspt.Description, 'N/A'),
		'DebitOrderBankName' = ISNULL(DebitBnk.ACBBankDescription, ''),
		'DebitOrderBankBranchName' = ISNULL(DebitBrn.ACBBranchDescription, ''),
		'DebitOrderBankAccountNumber' = ISNULL(DebitBank.AccountNumber, ''),
		'DebitOrderBankAccountName' = ISNULL(DebitBank.AccountName, ''),
		'DebitOrderBankAccountType' = ISNULL(DebitTyp.ACBTypeDescription, ''),
		'DebitOrderDay' = ISNULL(CONVERT(VARCHAR(3), DebitOrderDay), ''),
		'CorrespondenceType' = ISNULL(Cm.Description, 'N/A'),
		'CorrespondeceEmail' = ISNULL(CorresLe.EmailAddress, 'N/A'),
		'CorrespondeceAddress' = 
			CASE 
				WHEN oma.AddressKey IS NULL THEN 'N/A'
					ELSE ISNULL([2AM].dbo.fGetFormattedAddressDelimited(oma.AddressKey, 1),'N/A')
			END,
		'MarketingDetails' =ISNULL(Os.Description,'N/A'),
		'NumberofDependants' = ISNULL(Oml.DependentsPerHousehold, 0),
		'ContributingDependants' = ISNULL(Oml.ContributingDependents, 0),
		'TotalFees' = CONVERT(NUMERIC(13,2), Oivl.FeesTotal),
		'CapitaliseFees' =
			CASE
				WHEN Oa.OfferAttributeKey IS NOT NULL THEN 1
					ELSE 0
			END

FROM	[2AM].dbo.Offer O
		INNER JOIN (SELECT MAX(OfferInformationKey) as OIKey, OfferKey 
				FROM OfferInformation 
				GROUP BY OfferKey) OIKeys ON O.OfferKey=OIKeys.OfferKey 

		INNER JOIN [2AM].dbo.OfferInformation Oi ON
			(
				OIKeys.OIKey = OI.OfferInformationKey
			)
		INNER JOIN [2AM].dbo.OfferInformationVariableLoan Oivl ON
			(
				Oi.OfferInformationKey = Oivl.OfferInformationKey
			)

		LEFT OUTER JOIN [2AM].dbo.OfferAttribute Oa ON
			(
				O.OfferKey = Oa.OfferKey
					AND Oa.OfferAttributeTypeKey = 3
			)


		-- Property Details
		INNER JOIN [2AM].dbo.OfferMortgageLoan Oml ON
			(
				O.OfferKey = Oml.OfferKey
			)

		INNER JOIN [2AM].dbo.Property Prop ON
			(
				Oml.PropertyKey = Prop.PropertyKey
			)

		INNER JOIN [2AM].dbo.PropertyType Ptype ON
			(
				Prop.PropertyTypeKey = Ptype.PropertyTypeKey
			)

		INNER JOIN [2AM].dbo.TitleType Ttype ON
			(
				Prop.TitleTypeKey = Ttype.TitleTypeKey
			)

		INNER JOIN [2AM].dbo.OccupancyType Otype ON
			(
				Prop.OccupancyTypeKey = Otype.OccupancyTypeKey
			)
		LEFT OUTER JOIN [2AM].dbo.PropertyAccessDetails pad ON
			(
				pad.PropertyKey = prop.PropertyKey	
			)
		LEFT OUTER JOIN [2AM].dbo.Valuation Val ON
			(
				Prop.PropertyKey = Val.PropertyKey
				and Val.IsActive = 1
			)
		LEFT OUTER JOIN [2AM].dbo.Valuator Valuer ON
			(
				Val.ValuatorKey = Valuer.ValuatorKey
					AND Valuer.GeneralStatusKey = 1
			)

		LEFT OUTER JOIN [2AM].dbo.LegalEntity ValuerLe ON
			(
				Valuer.LegalEntityKey = ValuerLe.LegalEntityKey
			)


		-- Seller Details
		LEFT OUTER JOIN [2AM].dbo.OfferRole Seller ON
			(
				O.OfferKey = Seller.OfferKey
					AND Seller.GeneralStatusKey = 1
					AND Seller.OfferRoleTypeKey = 9
			)

		LEFT OUTER JOIN [2AM].dbo.LegalEntity SellerLe ON
			(
				Seller.LegalEntityKey = SellerLe.LegalEntityKey
			)


		-- Debit Order Details
		LEFT OUTER JOIN [2AM].dbo.OfferDebitOrder Odo ON
			(
				O.OfferKey = Odo.OfferKey 
			)
		LEFT OUTER JOIN [2AM].dbo.FinancialServicePaymentType Fspt ON
			(
				Odo.FinancialServicePaymentTypeKey = Fspt.FinancialServicePaymentTypeKey
			)

		LEFT OUTER JOIN [2AM].dbo.BankAccount DebitBank ON
			(
				Odo.BankAccountKey = DebitBank.BankAccountKey
			)

		LEFT OUTER JOIN [2AM].dbo.ACBBranch DebitBrn ON
			(
				DebitBank.ACBBranchCode = DebitBrn.ACBBranchCode
			)

		LEFT OUTER JOIN [2AM].dbo.ACBBank DebitBnk ON
			(
				DebitBrn.ACBBankCode = DebitBnk.ACBBankCode
			)

		LEFT OUTER JOIN [2AM].dbo.ACBType DebitTyp ON
			(
				DebitBank.ACBTypeNumber = DebitTyp.ACBTypeNumber
			)

		-- Correspondence Options
		LEFT OUTER JOIN [2AM].dbo.OfferMailingAddress oma ON
			(
				O.OfferKey = oma.OfferKey 
			)

		LEFT OUTER JOIN [2AM].dbo.CorrespondenceMedium Cm ON
			(
				oma.CorrespondenceMediumKey = Cm.CorrespondenceMediumKey
			)

		LEFT OUTER JOIN [2AM].dbo.LegalEntity CorresLe ON
			(
				oma.LegalEntityKey = CorresLe.LegalEntityKey
			)


		-- Expense Information
		LEFT OUTER JOIN (SELECT	i.OfferKey,
								'MonthlyPayment' = SUM(ISNULL(i.MonthlyPayment, 0))
						FROM	[2AM].dbo.OfferExpense i
						WHERE	i.ExpenseTypeKey = 8 -- Loan Account
						GROUP BY i.OfferKey, i.LegalEntityKey)	ExistLoan ON
			(
				O.OfferKey = ExistLoan.OfferKey
			)

		LEFT OUTER JOIN (SELECT	i.OfferKey,
								'MonthlyPayment' = SUM(ISNULL(i.MonthlyPayment, 0))
						FROM	[2AM].dbo.OfferExpense i
						WHERE	i.ExpenseTypeKey = 12 -- Vehicle Finance
						GROUP BY i.OfferKey, i.LegalEntityKey)	CarFin ON
			(
				O.OfferKey = CarFin.OfferKey
			)

		LEFT OUTER JOIN	(SELECT	i.OfferKey,
								'MonthlyPayment' = SUM(ISNULL(i.MonthlyPayment, 0))
						FROM	[2AM].dbo.OfferExpense i
						WHERE	i.ExpenseTypeKey = 11 -- Store Cards
						GROUP BY i.OfferKey, i.LegalEntityKey) Retail ON
			(
				O.OfferKey = Retail.OfferKey
			)

		LEFT OUTER JOIN (SELECT	i.OfferKey,
								'MonthlyPayment' = SUM(ISNULL(i.MonthlyPayment, 0))
						FROM	[2AM].dbo.OfferExpense i
						WHERE	i.ExpenseTypeKey = 13 -- Other
						GROUP BY i.OfferKey, i.LegalEntityKey)	OtherExp ON
			(
				O.OfferKey = OtherExp.OfferKey
			)


		-- Income Employment Details
		LEFT OUTER JOIN	(SELECT	Orl.OfferKey,
								'ConfirmedIncome' = SUM(ISNULL(Emp.ConfirmedIncome, 0)),
								'MonthlyIncome' = SUM(ISNULL(Emp.MonthlyIncome, 0)),
								'ConfirmedBasicIncome' = SUM(ISNULL(Emp.ConfirmedBasicIncome, 0)),
								'BasicIncome' = SUM(ISNULL(Emp.BasicIncome, 0)),
								'ConfirmedCommission' = SUM(ISNULL(Emp.ConfirmedCommission, 0)),
								'Commission' = SUM(ISNULL(Emp.Commission, 0)),
								'ConfirmedOvertime' = SUM(ISNULL(Emp.ConfirmedOvertime, 0)),
								'Overtime' = SUM(ISNULL(Emp.Overtime, 0)),
								'Other' = SUM(ISNULL(Emp.Shift, 0) + ISNULL(Emp.Performance, 0) + ISNULL(Emp.Allowances, 0)),
								'ConfirmedOther' = SUM(ISNULL(Emp.ConfirmedShift, 0) + ISNULL(Emp.ConfirmedPerformance, 0) + ISNULL(Emp.ConfirmedAllowances, 0))
						FROM	[2AM].dbo.OfferRole Orl
								INNER JOIN [2AM].dbo.Employment Emp ON
									(
										Orl.LegalEntityKey = Emp.LegalEntityKey
											AND Emp.EmploymentStatusKey = 1
									)
						WHERE	Orl.OfferRoleTypeKey IN (8, 11)
								AND Orl.GeneralStatusKey = 1
						GROUP BY Orl.OfferKey)	Emp ON
			(
				O.OfferKey = Emp.OfferKey
			)

		LEFT OUTER JOIN [2AM].dbo.OfferSource Os ON
			(
				O.OfferSourceKey = Os.OfferSourceKey
			)
		
WHERE	
	O.OfferKey = @OfferKey
ORDER BY val.ValuationDate desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
