    <DataSet Name="Assets">
      <Query>
        <DataSourceName>Real_time</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OfferKey">
            <Value>=Parameters!OfferKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dbo.legalentitylegalname(l.legalentitykey,1) as LEName,alt.description as [Type], 
case when al.AssetLiabilityTypeKey = 1 then dbo.fGetFormattedAddress(al.addresskey) 
when al.AssetLiabilityTypeKey = 2 then CompanyName 
when al.AssetLiabilityTypeKey = 6 then alst.description
else al.Description end as Description,
		'Date' =
			CASE
				WHEN Date IS NULL THEN ''
					ELSE CONVERT(VARCHAR(10), Al.Date, 103)
			END,
isnull(AssetValue,0) as [Asset Value], 
isnull(LiabilityValue,0) as [Liability Value], 
isnull(Cost,0) as Instalment
from 
offer o 
join offerrole orr on o.offerkey=orr.offerkey and offerroletypekey in (11,12)
join legalentityassetliability l on orr.legalentitykey=l.legalentitykey
join assetliability al on l.assetliabilitykey=al.assetliabilitykey
join assetliabilitytype alt on al.assetliabilitytypekey=alt.assetliabilitytypekey AND alt.Description not like 'Liability%'
left join assetliabilitysubtype alst on al.assetliabilitysubtypekey=alst.assetliabilitysubtypekey
where o.offerkey=@OfferKey
and l.GeneralStatusKey = 1
order by l.legalentitykey</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
