    <DataSet Name="AffordabilityIncome">
      <Query>
        <DataSourceName>Real_time</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OfferKey">
            <Value>=Parameters!OfferKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select at.description as [Income Type], sum(amount) as Amount
from offerrole orr join legalentityaffordability lea
on orr.legalentitykey=lea.legalentitykey
join affordabilitytype at on lea.affordabilitytypekey=at.affordabilitytypekey
where orr.offerkey=@OfferKey and lea.offerkey=@OfferKey  and offerroletypekey in (11,12) and isExpense=0
group by orr.offerkey,at.description</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
