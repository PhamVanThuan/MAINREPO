  <DataSets>
    <DataSet Name="AffordabilityAssessmentIncomeItems">
      <Query>
        <DataSourceName>Real_time</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AffordabilityAssessmentKey">
            <Value>=Parameters!AffordabilityAssessmentKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select at.[Description] as Income, aai.ClientValue as Amount
from [2AM].[dbo].[AffordabilityAssessmentItem] aai (nolock)
join [2AM].[dbo].[AffordabilityAssessmentItemType] at (nolock) on at.AffordabilityAssessmentItemTypeKey = aai.AffordabilityAssessmentItemTypeKey
	and at.AffordabilityAssessmentItemCategoryKey = 1 -- Income
where aai.AffordabilityAssessmentKey = @AffordabilityAssessmentKey
and aai.ClientValue is not null</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Income">
          <DataField>Income</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AffordabilityAssessmentExpenseItems">
      <Query>
        <DataSourceName>Real_time</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AffordabilityAssessmentKey">
            <Value>=Parameters!AffordabilityAssessmentKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select at.[Description] as Expense, aai.ClientValue as Amount
from [2AM].[dbo].[AffordabilityAssessmentItem] aai (nolock)
join [2AM].[dbo].[AffordabilityAssessmentItemType] at (nolock) on at.AffordabilityAssessmentItemTypeKey = aai.AffordabilityAssessmentItemTypeKey
	and at.AffordabilityAssessmentItemCategoryKey in (2, 3, 4, 6) -- Income Deductions, Necessary Expenses, Payment Obligations, Other Expenses
where aai.AffordabilityAssessmentKey = @AffordabilityAssessmentKey
and aai.ClientValue is not null</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Expense">
          <DataField>Expense</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AffordabilityAssessmentDetail">
      <Query>
        <DataSourceName>Real_time</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AffordabilityAssessmentKey">
            <Value>=Parameters!AffordabilityAssessmentKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select [2am].[dbo].fGetAffordabilityAssessmentContributors(@AffordabilityAssessmentKey, 0, 1) HouseHold, [NumberOfContributingApplicants], [NumberOfHouseholdDependants]
from [dbo].[AffordabilityAssessment] (nolock)
where AffordabilityAssessmentKey = @AffordabilityAssessmentKey</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="HouseHold">
          <DataField>HouseHold</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumberOfContributingApplicants">
          <DataField>NumberOfContributingApplicants</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NumberOfHouseholdDependants">
          <DataField>NumberOfHouseholdDependants</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
